{
    "name": "christopheraseidl/livewire-cookie-consent",
    "description": "Display a cookie consent notice and cookie policy in Laravel (Livewire + Alpine.js + Tailwind CSS).",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Seidl",
            "email": "christopheraseidl@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/christopheraseidl/livewire-cookie-consent"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "livewire/livewire": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.1"
    },
    "autoload": {
        "psr-4": {
            "christopheraseidl\\CookieConsent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "christopheraseidl\\CookieConsent\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/",
            "Workbench\\Database\\Factories\\": "workbench/database/factories/",
            "Workbench\\Database\\Seeders\\": "workbench/database/seeders/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "christopheraseidl\\CookieConsent\\CookieConsentServiceProvider"
            ],
            "aliases": {
                "CookieConsent": "christopheraseidl\\CookieConsent\\Facades\\CookieConsent"
            }
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@clear",
            "@prepare"
        ],
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": "@php vendor/bin/testbench workbench:build --ansi",
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "@build",
            "@php vendor/bin/testbench serve"
        ],
        "lint": [
            "@php vendor/bin/phpstan analyse"
        ],
        "test": [
            "@php vendor/bin/phpunit"
        ]
    }
}