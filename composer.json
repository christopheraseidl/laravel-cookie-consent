{
    "name": "christopheraseidl/livewire-cookie-consent",
    "description": "Display a cookie consent notice and cookie policy in Laravel (Livewire + Alpine.js + Tailwind CSS).",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Seidl",
            "email": "christopheraseidl@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/christopheraseidl/livewire-cookie-consent"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "livewire/livewire": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "christopheraseidl\\CookieConsent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "christopheraseidl\\CookieConsent\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "christopheraseidl\\CookieConsent\\CookieConsentServiceProvider"
            ],
            "aliases": {
                "CookieConsent": "christopheraseidl\\CookieConsent\\Facades\\CookieConsent"
            }
        }
    }
}